<% if end_users.exists? %>
  <table class='table'>
    <thead>
      <tr>
        <th>image</th>
        <th>nickname</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  <% end_users.each do |end_user| %>
    <tbody>
        <tr>
          <td><%= image_tag end_user.get_profile_image(100,80) %></td>
          <td><%= end_user.nickname %></td>
          <td>フォロー数: <%= end_user.followings.count %></td>
          <td>フォロワー数: <%= end_user.followers.count %></td>
        <td><%= link_to 'Show', end_user %></td>
        </tr>
    </tbody>
  <% end %>
  </table>
<% else %>
  <p>ユーザーはいません</p>
<% end %>