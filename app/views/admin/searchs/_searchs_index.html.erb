
<div class="row-justify-content-center">
  <table class='table table-hover table-inverse col-md-12'>
    <thead>
      <tr>
        <th colspan="3"><%= search_name %></th>
        <th colspan="2"></th>
      </tr>
    </thead>
    <tbody>
      <% searchs.each do |search| %>
      <tr>
        <td>
          <%= search.name %>
        </td>
        <td>
          <%= link_to "編集する", edit_admin_search_path(search.id), class: "btn btn-success" %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>